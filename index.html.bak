            "7710": {
                id: "7710", rank: 4081, fur: "Yellow", pattern: "Solid", eyes: "Sleepy Eyes", clothes: "None", realm: "Sunward Heights", order: "Flame", hatColor: "Orange",
                suggestedName: "Goldstrike the Lightning-Fast",
