            "2426": {
                id: "2426", rank: 2082, fur: "White", pattern: "Blotted", eyes: "Black Eyes", clothes: "Turtleneck", realm: "Shadowmire", order: "Wild", hatColor: "Green",
                suggestedName: "Blotbark the Corruption-Fighter",
