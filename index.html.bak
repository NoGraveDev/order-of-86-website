        // Search functionality
        function searchDog() {
            const input = document.getElementById('searchInput');
            const dogCard = document.getElementById('dogCard');
            const errorMessage = document.getElementById('errorMessage');
            const dogId = input.value.replace('#', '').trim();
            
            if (!dogId) {
                dogCard.style.display = 'none';
                errorMessage.style.display = 'none';
                return;
            }
            
            const dog = wizardDogs[dogId];
            
            if (dog) {
                displayDog(dog);
                dogCard.style.display = 'block';
                errorMessage.style.display = 'none';
            } else {
                dogCard.style.display = 'none';
                errorMessage.style.display = 'block';
            }
        }

        function displayDog(dog) {
            // Update card styling based on order
            const dogCard = document.getElementById('dogCard');
            dogCard.className = 'dog-card ' + dog.order.toLowerCase().replace(/[^a-z]/g, '');
            
            // Update image
            document.getElementById('dogImage').src = dog.image;
            
            // Update basic info
            document.getElementById('dogName').textContent = dog.suggestedName;
            document.getElementById('dogTitle').textContent = `#${dog.id} (Rank ${dog.rank})`;
            document.getElementById('dogId').textContent = `#${dog.id}`;
            
            // Update order badge
            const orderBadge = document.getElementById('orderBadge');
            orderBadge.textContent = dog.order;
            orderBadge.className = 'order-badge ' + dog.order.toLowerCase().replace(/[^a-z]/g, '');
            
            // Update metadata
            document.getElementById('dogRank').textContent = dog.rank.toLocaleString();
            document.getElementById('dogFur').textContent = dog.fur;
            document.getElementById('dogPattern').textContent = dog.pattern;
            document.getElementById('dogEyes').textContent = dog.eyes;
            document.getElementById('dogClothes').textContent = dog.clothes;
            document.getElementById('dogRealm').textContent = dog.realm;
            
            // Pre-fill editable fields
            document.getElementById('dogNameInput').value = dog.suggestedName;
            document.getElementById('dogStoryInput').value = dog.suggestedStory;
            
            // Update hidden form fields
            document.getElementById('formDogId').value = dog.id;
            document.getElementById('formRank').value = dog.rank;
            document.getElementById('formFur').value = dog.fur;
            document.getElementById('formPattern').value = dog.pattern;
            document.getElementById('formEyes').value = dog.eyes;
            document.getElementById('formClothes').value = dog.clothes;
            document.getElementById('formRealm').value = dog.realm;
            document.getElementById('formOrder').value = dog.order;
        }

        // Event listeners
        document.getElementById('searchInput').addEventListener('input', searchDog);
        document.getElementById('searchInput').addEventListener('keypress', function(e) {
            if (e.key === 'Enter') {
                searchDog();
            }
        });

        // Form submission handling
        document.getElementById('dogForm').addEventListener('submit', function(e) {
            // Form will submit normally to FormSubmit.co
            // No need to prevent default
        });
    </script>
