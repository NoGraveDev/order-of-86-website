            "8667": {
                id: "8667", rank: 3969, fur: "Orange", pattern: "Solid", eyes: "Sleepy Eyes", clothes: "None", realm: "Sunward Heights", order: "Flame", hatColor: "Orange",
                suggestedName: "Goldforge the Sun-Touched",
